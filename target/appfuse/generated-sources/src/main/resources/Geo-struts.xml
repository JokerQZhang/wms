        <action name="geoes" class="com.joker.wms.webapp.action.GeoAction" method="list">
            <result>/WEB-INF/pages/geoList.jsp</result>
        </action>

        <action name="editGeo" class="com.joker.wms.webapp.action.GeoAction" method="edit">
            <result>/WEB-INF/pages/geoForm.jsp</result>
            <result name="error">/WEB-INF/pages/geoList.jsp</result>
        </action>

        <action name="saveGeo" class="com.joker.wms.webapp.action.GeoAction" method="save">
            <result name="input">/WEB-INF/pages/geoForm.jsp</result>
            <result name="cancel" type="redirectAction">geoes</result>
            <result name="delete" type="redirectAction">geoes</result>
            <result name="success" type="redirectAction">geoes</result>
        </action>
